{
  "name": "kps-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "migrate-contracts": "tools/migrate.sh",
    "serve": "vue-cli-service serve --mode=development",
    "serve-blockchain": "truffle develop --network=local",
    "build": "vue-cli-service build",
    "test": "truffle test",
    "lint": "solhint contracts/*.sol && vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2",
    "web3": "^1.2.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "solhint": "^2.3.0",
    "truffle": "^5.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
